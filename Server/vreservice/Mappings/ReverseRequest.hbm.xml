<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="vreservice" namespace="Vre.Server.BusinessLogic">
  <class name="ReverseRequest" table="ReverseRequests" dynamic-update="true" lazy="false">

    <id name="Id" column="ID" unsaved-value="00000000-0000-0000-0000-000000000000">
      <generator class="assigned"/>
    </id>

    <property name="Request" column="Request" />
    <property name="ExpiresOn" column="ExpiresOn" />

    <property name="UserId" column="UserId" />
    <property name="Login" column="Login" />
    <property name="Subject" column="Subject" />

    <property name="ReferenceParamName" column="ReferenceParamName" />
    <property name="ReferenceParamValue" column="ReferenceParamValue" />

  </class>
</hibernate-mapping>
