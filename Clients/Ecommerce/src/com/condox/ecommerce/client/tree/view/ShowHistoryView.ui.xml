<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			color: mediumblue;
			margin-left: 50px;
			background-color: slateblue;
		}
	</ui:style>
		<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel>
				<g:VerticalPanel spacing="5">
					<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
						<g:Hyperlink text="Close" targetHistoryToken="" ui:field="linkClose"/>
					</g:HorizontalPanel>
					<g:ScrollPanel width="400px" height="200px">
						<g:HTML wordWrap="true" width="100%" height="100%" ui:field="htmlHistory"/>
					</g:ScrollPanel>
			</g:VerticalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 