<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel height="100%">
		<g:VerticalPanel width="100%" height="100%" spacing="30">
			<g:HTML wordWrap="true" ui:field="summary"/>
			<g:Cell verticalAlignment="ALIGN_BOTTOM" horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel spacing="10" verticalAlignment="ALIGN_MIDDLE">
				<g:Button width="75px" text="Delete" ui:field="delete"/>
				<g:CheckBox text="Enabled" ui:field="enable"/>
			</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 