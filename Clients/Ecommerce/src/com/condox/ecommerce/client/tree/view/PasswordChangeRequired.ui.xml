<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="100%" height="100%">
			<g:HorizontalPanel spacing="10" width="100%">
				<g:Image width="100px" height="100px" url="images/ChangePasswordRequired.jpg"/>
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:Label text="Please change your password" width="133px"/>
				</g:Cell>
			</g:HorizontalPanel>
			<g:Cell verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel spacing="10" width="100%">
				<g:Button text="Change now..." width="120px" ui:field="now"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Button text="Remind me later" width="120px" ui:field="later"/>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 