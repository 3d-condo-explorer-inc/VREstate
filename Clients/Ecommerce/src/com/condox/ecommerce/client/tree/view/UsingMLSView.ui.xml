<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			color: mediumblue;
			margin-left: 50px;
			background-color: slateblue;
		}
	</ui:style>
	<g:VerticalPanel width="100%" height="100%"
		horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
		<g:VerticalPanel>
			<g:VerticalPanel spacing="5" width="300px" height="200px">
				<g:HorizontalPanel spacing="20">
					<g:Label text="Published with MLS?"/>
				</g:HorizontalPanel>
				<g:VerticalPanel spacing="10">
					<g:RadioButton name="new name" text="MLS#" ui:field="rbMLS"/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:TextBox ui:field="textMLS"/>
					</g:Cell>
					<g:RadioButton name="new name" text="This is a private listing, so let&apos;s submit all info manually" checked="true" ui:field="rbAddress"/>
				</g:VerticalPanel>
				<g:Cell verticalAlignment="ALIGN_BOTTOM">
					<g:HorizontalPanel width="100%">
						<g:Cell width="50%" verticalAlignment="ALIGN_MIDDLE">
							<g:Button text="Cancel" ui:field="buttonCancel" width="75px"/>
						</g:Cell>
						<g:HorizontalPanel spacing="5">
							<g:Button width="75px" ui:field="buttonPrev" text="&lt;&lt; Prev">Prev</g:Button>
							<g:Button text="Next &gt;&gt;" ui:field="buttonNext" width="75px"/>
						</g:HorizontalPanel>
					</g:HorizontalPanel>
				</g:Cell>

			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 