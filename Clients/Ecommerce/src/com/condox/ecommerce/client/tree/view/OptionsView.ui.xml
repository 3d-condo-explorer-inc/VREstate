<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			color: mediumblue;
			margin-left: 50px;
			background-color: slateblue;
		}
	</ui:style>
		<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel borderWidth="3">
				<g:VerticalPanel spacing="5" width="300px">
					<g:Label text="Options of the Interactive 3D Listing" horizontalAlignment="ALIGN_CENTER"/>
					<g:HorizontalPanel width="100%">
						<g:Cell width="40%" horizontalAlignment="ALIGN_CENTER">
							<g:Label text="New Label" ui:field="strMLS"/>
						</g:Cell>
						<g:Cell width="30%">
							<g:RadioButton name="new name" text="For Sale" ui:field="rbForSale" checked="true"/>
						</g:Cell>
						<g:Cell width="30%">
							<g:RadioButton name="new name" text="For Rent" ui:field="rbForRent"/>
						</g:Cell>
					</g:HorizontalPanel>
					<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
						<g:HorizontalPanel spacing="10">
							<g:Label text="Price&#9;"/>
						</g:HorizontalPanel>
						<g:Cell horizontalAlignment="ALIGN_RIGHT" width="100px">
							<g:TextBox width="100px" ui:field="textPrice"/>
						</g:Cell>
					</g:HorizontalPanel>
					<g:Label text="Third Party Virtual  Tour (optional)"/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:TextBox ui:field="textVirtualTourUrl" width="250px"/>
					</g:Cell>
				<g:Label text="More Info link (optional)"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:TextBox ui:field="textMoreInfoUrl" width="250px"/>
				</g:Cell>
				<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:Cell verticalAlignment="ALIGN_MIDDLE">
						<g:Button width="75px" ui:field="buttonCancel">Cancel</g:Button>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HorizontalPanel spacing="5">
						<g:Button text="&lt;&lt; Prev" width="75px" ui:field="buttonPrev">Prev</g:Button>
						<g:Button width="75px" ui:field="buttonNext">Next >></g:Button>
					</g:HorizontalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 