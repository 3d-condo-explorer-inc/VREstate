<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			color: mediumblue;
			margin-left: 50px;
			background-color: slateblue;
		}
		.bg {
			color: Blue;
			background-color: Teal;
		}
	</ui:style>
		<g:VerticalPanel width="750px" height="500px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="100%" horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER" width="100%" spacing="10">
					<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:Label text="New Label" ui:field="listingType"/>
					</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Image height="164px" width="194px" ui:field="image"/>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Cell width="50%">
				<g:HorizontalPanel spacing="20">
						<g:VerticalPanel>
							<g:HorizontalPanel spacing="5" width="100%">
								<g:Cell verticalAlignment="ALIGN_MIDDLE">
									<g:Label text="MLS#" horizontalAlignment="ALIGN_LEFT"/>
								</g:Cell>
								<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
									<g:TextBox width="100px" ui:field="textMLS"/>
								</g:Cell>
							</g:HorizontalPanel>
							<g:Cell width="100%" horizontalAlignment="ALIGN_RIGHT">
								<g:HorizontalPanel spacing="5" horizontalAlignment="ALIGN_RIGHT" width="100%">
								<g:Cell verticalAlignment="ALIGN_MIDDLE">
									<g:Label text="Price, $" horizontalAlignment="ALIGN_LEFT" ui:field="priceCaption"/>
								</g:Cell>
								<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
									<g:TextBox ui:field="textPrice" width="100px"/>
								</g:Cell>
							</g:HorizontalPanel>
							</g:Cell>
						</g:VerticalPanel>
						<g:Cell height="">
							<g:VerticalPanel spacing="10">
							<g:RadioButton name="new name" text="For Sale" ui:field="rbForSale" checked="true"/>
							<g:RadioButton name="new name" text="For Rent" ui:field="rbForRent"/>
						</g:VerticalPanel>
						</g:Cell>
					</g:HorizontalPanel>
			</g:Cell>
			<g:Label text="Third Party Virtual  Tour (optional)" horizontalAlignment="ALIGN_LEFT" width="400px"/>
			<g:TextBox ui:field="textVirtualTourUrl" width="400px" value="http://"/>
			<g:Label text="More Info link (optional)" width="400px" horizontalAlignment="ALIGN_LEFT"/>
			<g:TextBox ui:field="textMoreInfoUrl" width="400px" value="http://"/>
			<g:Cell verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" spacing="10" verticalAlignment="ALIGN_BOTTOM">
					<g:Cell verticalAlignment="ALIGN_MIDDLE">
						<g:Button width="75px" ui:field="buttonCancel">Cancel</g:Button>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HorizontalPanel spacing="5">
						<g:Button text="&lt;&lt; Prev" width="75px" ui:field="buttonPrev">Prev</g:Button>
						<g:Button width="75px" ui:field="buttonNext">Next >></g:Button>
					</g:HorizontalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
</ui:UiBinder> 