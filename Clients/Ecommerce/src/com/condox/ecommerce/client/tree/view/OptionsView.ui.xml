<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			color: mediumblue;
			margin-left: 50px;
			background-color: slateblue;
		}
	</ui:style>
		<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" styleName="incorrect">
			<g:VerticalPanel >
				<g:VerticalPanel spacing="5" width="300px">
					<g:Label text="Options of the Interactive 3D Listing" horizontalAlignment="ALIGN_CENTER"/>
					<g:HorizontalPanel width="100%">
						<g:VerticalPanel>
							<g:HorizontalPanel spacing="5" width="100%">
								<g:Cell verticalAlignment="ALIGN_MIDDLE">
									<g:Label text="MLS#" horizontalAlignment="ALIGN_LEFT"/>
								</g:Cell>
								<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
									<g:TextBox width="100px" ui:field="textMLS"/>
								</g:Cell>
							</g:HorizontalPanel>
							<g:Cell width="100%" horizontalAlignment="ALIGN_RIGHT">
								<g:HorizontalPanel spacing="5" horizontalAlignment="ALIGN_RIGHT" width="100%">
								<g:Cell verticalAlignment="ALIGN_MIDDLE">
									<g:Label text="Price, $" horizontalAlignment="ALIGN_LEFT" ui:field="priceCaption"/>
								</g:Cell>
								<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
									<g:TextBox ui:field="textPrice" width="100px"/>
								</g:Cell>
							</g:HorizontalPanel>
							</g:Cell>
						</g:VerticalPanel>
						<g:Cell height="">
							<g:VerticalPanel spacing="10">
							<g:RadioButton name="new name" text="For Sale" ui:field="rbForSale" checked="true"/>
							<g:RadioButton name="new name" text="For Rent" ui:field="rbForRent"/>
						</g:VerticalPanel>
						</g:Cell>
					</g:HorizontalPanel>
					<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE"/>
					<g:Label text="Third Party Virtual  Tour (optional)"/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:TextBox ui:field="textVirtualTourUrl" width="250px" value="http://"/>
					</g:Cell>
				<g:Label text="More Info link (optional)"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:TextBox ui:field="textMoreInfoUrl" width="250px" value="http://"/>
				</g:Cell>
				<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
					<g:Cell verticalAlignment="ALIGN_MIDDLE">
						<g:Button width="75px" ui:field="buttonCancel">Cancel</g:Button>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HorizontalPanel spacing="5">
						<g:Button text="&lt;&lt; Prev" width="75px" ui:field="buttonPrev">Prev</g:Button>
						<g:Button width="75px" ui:field="buttonNext">Next >></g:Button>
					</g:HorizontalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 