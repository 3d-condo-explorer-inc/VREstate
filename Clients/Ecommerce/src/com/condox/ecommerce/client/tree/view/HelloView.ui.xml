<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	
	</ui:style>
			<g:VerticalPanel spacing="10" ui:field="mainView">
			<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
				<g:Label text="New Label" horizontalAlignment="ALIGN_LEFT" ui:field="userName"/>
				<g:Button text="Logout" ui:field="logout"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="10">
				<g:Button width="200px" text="Update your profile..." ui:field="editProfile"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Button width="200px" text="Settings..." ui:field="editSettings"/>
				</g:Cell>
			</g:HorizontalPanel>
			<g:HorizontalPanel width="800px" borderWidth="2">
				<g:VerticalPanel spacing="10" width="300px">
					<g:ScrollPanel ui:field="scroll" height="400px">
						<p1:CellList ui:field="orderList" width="100%" height="100%" />
					</g:ScrollPanel>
			</g:VerticalPanel>
			<g:Cell width="100%">
				<g:HTMLPanel height="100%" ui:field="details"/>
			</g:Cell>
		</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="10">
				<g:Button width="200px" text="Create new ViewOrder..." ui:field="createOrder"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Button width="200px" text="Show history..." ui:field="showHistory"/>
				</g:Cell>
			</g:HorizontalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 