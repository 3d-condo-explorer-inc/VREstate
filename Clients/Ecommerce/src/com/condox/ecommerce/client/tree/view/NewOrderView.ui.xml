<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			color: mediumblue;
			margin-left: 50px;
			background-color: slateblue;
		}
	</ui:style>
	<g:VerticalPanel width="100%" height="100%"
		horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
		<g:VerticalPanel borderWidth="3">
			<g:VerticalPanel spacing="5" width="250px" height="200px">
				<g:HorizontalPanel spacing="10">
					<g:Label text="Pick a product:"/>
				</g:HorizontalPanel>
				<g:VerticalPanel spacing="10">
					<g:RadioButton name="new name" text="Interactive 3D Listing" checked="true"/>
					<g:RadioButton name="new name" text="Interactive 3D Layout of a Building" enabled="false"/>
				</g:VerticalPanel>
				<g:Cell verticalAlignment="ALIGN_BOTTOM">
					<g:HorizontalPanel width="100%">
						<g:Cell width="50%">
							<g:Button text="Cancel" ui:field="buttonCancel"/>
						</g:Cell>
						<g:Cell width="50%" horizontalAlignment="ALIGN_RIGHT">
							<g:Button text="Next &gt;&gt;" ui:field="buttonNext"/>
						</g:Cell>
					</g:HorizontalPanel>
				</g:Cell>

			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 