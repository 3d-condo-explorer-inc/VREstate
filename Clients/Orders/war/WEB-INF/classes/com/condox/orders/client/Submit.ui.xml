<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="300px" height="300px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" width="320px" height="300px">
					<g:Label text="If you want to order this listing, please submit this form" horizontalAlignment="ALIGN_CENTER"/>
					<g:HorizontalPanel width="100%" spacing="10">
				<g:Cell width="50%">
					<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER" spacing="10" height="100%" verticalAlignment="ALIGN_MIDDLE">
					<g:Label text="Your Name*" width="100%" horizontalAlignment="ALIGN_LEFT"/>
					<g:TextBox alignment="LEFT" width="100%" height="16px" ui:field="txtName"/>
					<g:Label text="Contact Phone*" horizontalAlignment="ALIGN_RIGHT" width="100%"/>
				</g:VerticalPanel>
				</g:Cell>
				<g:Cell width="50%">
					<g:VerticalPanel width="100%" spacing="10" height="100%" verticalAlignment="ALIGN_MIDDLE">
					<g:Label text="Email*" horizontalAlignment="ALIGN_LEFT"/>
					<g:TextBox width="100%" ui:field="txtMail"/>
					<g:TextBox ui:field="txtPhone"/>
				</g:VerticalPanel>
				</g:Cell>
			</g:HorizontalPanel>
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP" height="101px">
						<g:Label text="What type of Listing do you want to order?"/>
						<g:RadioButton name="new name" text="Private Interactive 3D Listing" checked="true" ui:field="rbPrivateListing"/>
						<g:RadioButton name="new name" text="Shared Interactive 3D Listing" ui:field="rbSharedListing" checked="true"/>
					</g:VerticalPanel>
					</g:Cell>
					<g:Label horizontalAlignment="ALIGN_LEFT" height="60px"/>
					<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_BOTTOM">
				<g:Button width="100px" text="Cancel" ui:field="btnCancel" styleName="submit-btnCancel" height="32px"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Button width="100px" text="Submit" ui:field="btnSubmit" enabled="false" styleName="submit-btnCancel" height="32px"/>
				</g:Cell>
			</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 