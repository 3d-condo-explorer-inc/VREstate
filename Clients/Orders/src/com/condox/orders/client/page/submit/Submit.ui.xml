<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.myTextBox {
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" spacing="10">
					<g:HTML wordWrap="true" horizontalAlignment="ALIGN_CENTER">If you want to order this listing, please submit this form <br></br> and we'll contact you shortly</g:HTML>
					<g:HorizontalPanel spacing="10" width="100%">
						<g:VerticalPanel spacing="10" width="100%" horizontalAlignment="ALIGN_LEFT">
							<g:Label text="Your Name*" width="100%" horizontalAlignment="ALIGN_LEFT"/>
							<g:TextBox alignment="LEFT" ui:field="txtName" visibleLength="1" width="125px"/>
							<g:Cell horizontalAlignment="ALIGN_RIGHT">
								<g:HTML wordWrap="true" width="125px" horizontalAlignment="ALIGN_RIGHT">Contact phone* <br></br> (including area code)</g:HTML>
							</g:Cell>
						</g:VerticalPanel>
						<g:VerticalPanel width="100%" spacing="10">
							<g:Label text="Email*" horizontalAlignment="ALIGN_LEFT" width="100%"/>
							<g:TextBox ui:field="txtMail" visibleLength="1" width="175px"/>
							<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_MIDDLE">
								<g:TextBox ui:field="txtPhone" visibleLength="1" width="100px"/>
								<g:Label text="Ext." horizontalAlignment="ALIGN_RIGHT" width="100%"/>
								<g:Cell horizontalAlignment="ALIGN_RIGHT">
									<g:TextBox ui:field="txtExtPhone" visibleLength="1" width="30px"/>
								</g:Cell>
						</g:HorizontalPanel>
						</g:VerticalPanel>
					</g:HorizontalPanel>
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP" height="101px">
						<g:Label text="What type of Listing do you want to order?"/>
						<g:RadioButton name="new name" text="Private Interactive 3D Listing" checked="true" ui:field="rbPrivateListing"/>
						<g:RadioButton name="new name" text="Shared Interactive 3D Listing" ui:field="rbSharedListing"/>
					</g:VerticalPanel>
					</g:Cell>
					<g:Label height="1em"/>
					<g:Cell verticalAlignment="ALIGN_BOTTOM" height="">
						<g:HTML wordWrap="true" height="50px" width="100%" horizontalAlignment="ALIGN_CENTER" ui:field="urlFloorplan"><A href="www.google.com">Floor plan</A></g:HTML>
					</g:Cell>
					<g:HTML wordWrap="true" styleName="my-action">All orders are free if submitted before April 15th 2013.<br /> Then it'll be just $49.99!</g:HTML>
					<g:Label horizontalAlignment="ALIGN_LEFT" height="36px"/>
					<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_BOTTOM">
				<g:Button width="100px" text="Cancel" ui:field="btnCancel" styleName="submit-btnCancel" height="32px"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:Button width="100px" text="Submit" ui:field="btnSubmit" enabled="false" styleName="submit-btnCancel" height="32px"/>
				</g:Cell>
			</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 