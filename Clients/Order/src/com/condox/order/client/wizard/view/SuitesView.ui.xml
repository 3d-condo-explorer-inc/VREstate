<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:DockLayoutPanel unit="EM" width="100%" height="100%" ui:field="dockPanel">
			<g:north size="4.0">
				<g:VerticalPanel width="100%">
					<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" spacing="5">
						<g:Button text="&lt;&lt; Prev" ui:field="buttonPrev"/>
						<g:Cell horizontalAlignment="ALIGN_RIGHT">
							<g:Button ui:field="buttonNext" text="Next &gt;&gt;" enabled="false"/>
						</g:Cell>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:north>
			<g:center>
				<p1:DataGrid ui:field="dataGrid"/>
			</g:center>
		</g:DockLayoutPanel>
		
		
</ui:UiBinder> 