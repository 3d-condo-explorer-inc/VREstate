<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			margin-left: 25px;
			font-weight: normal;
		}
	</ui:style>
		<g:DockLayoutPanel unit="EM">
			<g:north size="4.0">
				<g:HorizontalPanel width="100%" spacing="5">
					<g:Button ui:field="buttonPrev" text="&lt;&lt; Prev"/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:Button ui:field="buttonNext" text="Next &gt;&gt;"/>
					</g:Cell>
				</g:HorizontalPanel>
			</g:north>
			<g:center>
				<g:LayoutPanel>
					<g:layer>
						<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel>
				<g:Label text="Product"/>
				<g:RadioButton name="Listing/Layout" text="Interactive 3D Listing" ui:field="rbListing" checked="true"/>
				<g:RadioButton name="Private/Shared" text="Private" styleName="{style.important}" ui:field="rbListingPrivate" checked="true"/>
				<g:RadioButton name="Private/Shared" text="Shared" styleName="{style.important}" ui:field="rbListingShared"/>
				<g:RadioButton name="Listing/Layout" text="Interactive 3D Layout" ui:field="rbLayout"/>
			</g:VerticalPanel>
		</g:VerticalPanel>
					</g:layer>
				</g:LayoutPanel>
			</g:center>
		</g:DockLayoutPanel>
</ui:UiBinder> 