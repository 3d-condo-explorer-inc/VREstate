<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			margin-left: 25px;
			font-weight: normal;
		}
	</ui:style>
					<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:VerticalPanel>
							<g:VerticalPanel spacing="10">
						<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel>
				<g:Label text="Product"/>
				<g:RadioButton name="Listing/Layout" text="Interactive 3D Listing" ui:field="rbListing" checked="true"/>
				<g:RadioButton name="Private/Shared" text="Private" styleName="{style.important}" ui:field="rbListingPrivate" checked="true"/>
				<g:RadioButton name="Private/Shared" text="Shared" styleName="{style.important}" ui:field="rbListingShared"/>
				<g:RadioButton name="Listing/Layout" text="Interactive 3D Layout" ui:field="rbLayout"/>
			</g:VerticalPanel>
		</g:VerticalPanel>
					</g:VerticalPanel>
						</g:VerticalPanel>
									<g:Cell verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_BOTTOM">
					<g:HorizontalPanel spacing="10">
						<g:Button text="Cancel" width="100px" ui:field="buttonCancel"/>
					</g:HorizontalPanel>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HorizontalPanel spacing="10">
						<g:Button text="&lt;&lt; Prev" width="100px" ui:field="buttonPrev"/>
						<g:Button text="Next &gt;&gt;" width="100px" ui:field="buttonNext"/>
					</g:HorizontalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
					</g:VerticalPanel>
</ui:UiBinder> 