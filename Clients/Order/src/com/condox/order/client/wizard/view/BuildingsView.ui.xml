<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
				<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:VerticalPanel>
						<g:VerticalPanel spacing="10">
							<g:HorizontalPanel width="100%">
						<g:HTML wordWrap="true" width="430px">Please select a building from the list below, and click Select the Suite button next to the building</g:HTML>
					</g:HorizontalPanel>
							<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_LEFT" width="100%">
				<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE"
					spacing="3">
					<g:TextBox visibleLength="40" ui:field="textFilter" />
					<g:Cell verticalAlignment="ALIGN_MIDDLE">
						<g:ListBox selectedIndex="0" width="140px" ui:field="city" />
					</g:Cell>
				</g:HorizontalPanel>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:HorizontalPanel spacing="10">
						<g:Hyperlink
							targetHistoryToken="" ui:field="hyperlink" styleName="my-hyperlink" />
					</g:HorizontalPanel>
				</g:Cell>
			</g:HorizontalPanel>
							<p1:DataGrid ui:field="dataGrid" height="300px" width="700px" />
							<g:HTMLPanel>
				<table style="width:100%">
					<tr>
						<td align='center'>
							<c:SimplePager ui:field='pager' />
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
						</g:VerticalPanel>
					</g:VerticalPanel>
					<g:Cell verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_BOTTOM">
					<g:HorizontalPanel spacing="10">
						<g:Button text="Cancel" width="100px" ui:field="buttonCancel"/>
					</g:HorizontalPanel>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HorizontalPanel spacing="10" width="230px">
						<g:Button text="&lt;&lt; Prev" width="100px" ui:field="buttonPrev"/>
					</g:HorizontalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
				</g:VerticalPanel>
</ui:UiBinder> 