<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:HorizontalPanel width="100%" height="100%" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" spacing="10">
				<g:Label text="Select suite:"/>
				<g:ListBox visibleItemCount="5" ui:field="listSuites" width="150px" height="350px"/>
				<g:HorizontalPanel width="150px" spacing="5" horizontalAlignment="ALIGN_CENTER">
					<g:Button text="&lt; Prev" ui:field="btnPrev" width="100%"/>
					<g:Button text="Next &gt;" ui:field="btnNext" width="100%"/>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:HorizontalPanel>
</ui:UiBinder> 