<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
			<g:DockLayoutPanel unit="PX">
				<g:north size="50.0">
					<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" styleName="my-header" horizontalAlignment="ALIGN_LEFT" width="100%">
						<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" spacing="3">
							<g:TextBox visibleLength="40" ui:field="textFilter"/>
						</g:HorizontalPanel>
						<g:Cell horizontalAlignment="ALIGN_RIGHT">
							<g:HorizontalPanel spacing="10">
							<g:Hyperlink text="SELECT SUITE &gt;&gt;" targetHistoryToken="" ui:field="hyperlink" styleName="my-hyperlink"/>
						</g:HorizontalPanel>
						</g:Cell>
					</g:HorizontalPanel>
				</g:north>
				<g:center>
					<p1:DataGrid ui:field="dataGrid"/>
				</g:center>
			</g:DockLayoutPanel>
</ui:UiBinder> 