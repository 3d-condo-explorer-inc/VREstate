<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:HorizontalPanel width="100%" height="100%" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER">
				<g:CaptionPanel captionText="Order type:" width="200px">
					<g:HorizontalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" spacing="10">
						<g:RadioButton name="new name" text="Private" checked="true" ui:field="rbPrivate"/>
						<g:RadioButton name="new name" text="Public" ui:field="rbPublic"/>
					</g:HorizontalPanel>
				</g:CaptionPanel>
				<g:Cell horizontalAlignment="ALIGN_CENTER">
					<g:HorizontalPanel width="200px" horizontalAlignment="ALIGN_CENTER" spacing="10">
						<g:Button width="100%" text="&lt; Prev" ui:field="button"/>
					<g:Button width="100%" text="Next &gt;" ui:field="btnNext"/>
				</g:HorizontalPanel>
				</g:Cell>
			</g:VerticalPanel>
		</g:HorizontalPanel>
</ui:UiBinder> 