<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
			<g:DockLayoutPanel unit="PX">
				<g:north size="50.0">
					<g:HorizontalPanel height="100%" styleName="my-header" verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_LEFT" width="100%" spacing="10">
						<g:HorizontalPanel horizontalAlignment="ALIGN_LEFT">
							<g:Hyperlink text="&lt;&lt; CANCEL" targetHistoryToken="newHistoryToken" styleName="my-hyperlink" ui:field="hyperlink"/>
						</g:HorizontalPanel>
						<g:Cell horizontalAlignment="ALIGN_RIGHT">
							<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT">
								<g:Cell horizontalAlignment="ALIGN_RIGHT">
									<g:Hyperlink text="SUBMIT &gt;&gt;" targetHistoryToken="newHistoryToken" styleName="my-hyperlink" ui:field="hyperlink_1"/>
								</g:Cell>
						</g:HorizontalPanel>
						</g:Cell>
						<g:Cell horizontalAlignment="ALIGN_RIGHT">
							<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT">
								<g:HTML wordWrap="true" ui:field="htmlPayPal"/>
							</g:HorizontalPanel>
						</g:Cell>
					</g:HorizontalPanel>
				</g:north>
				<g:center>
					<g:ScrollPanel>
						<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" width="100%" height="100%">
							<g:HorizontalPanel width="100%" height="99px"/>
						<g:Cell horizontalAlignment="ALIGN_CENTER">
							<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" borderWidth="2">
								<g:Cell horizontalAlignment="ALIGN_CENTER">
									<g:VerticalPanel horizontalAlignment="ALIGN_CENTER">
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:Label text="If you want to order this listing, please submit the form and we&apos;ll contact you shortly." width="250px"/>
					</g:Cell>
					<g:HorizontalPanel width="100%" height="40px"/>
					<g:HorizontalPanel>
						<g:VerticalPanel spacing="10">
							<g:Label text="Your name*"/>
							<g:TextBox width="160px" ui:field="textName" text="MyName"/>
							<g:HorizontalPanel spacing="5" width="100%" horizontalAlignment="ALIGN_RIGHT">
								<g:VerticalPanel horizontalAlignment="ALIGN_RIGHT">
									<g:Label text="Contact phone*"/>
									<g:Label text="(including area code)"/>
								</g:VerticalPanel>
							</g:HorizontalPanel>
						</g:VerticalPanel>
						<g:VerticalPanel spacing="10">
							<g:Label text="Email*"/>
							<g:TextBox width="180px" ui:field="textEmail" text="eugene.simonov@3dcondox.com"/>
							<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE">
								<g:TextBox width="100px" ui:field="textPhone" text="1234567890"/>
								<g:Cell horizontalAlignment="ALIGN_RIGHT">
									<g:Label text="Ext. " width="40px"/>
								</g:Cell>
								<g:Cell width="40px" horizontalAlignment="ALIGN_RIGHT">
									<g:TextBox visibleLength="5" width="30px" ui:field="textPhoneExt" text="1234"/>
								</g:Cell>
							</g:HorizontalPanel>
						</g:VerticalPanel>
					</g:HorizontalPanel>
					<g:HorizontalPanel width="100%" height="40px"/>
					<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER" spacing="10">
						<g:Label text="What type of Listing do you want to order?"/>
						<g:RadioButton name="new name" text="Private Interactive 3D Listing" ui:field="rbPrivate"/>
						<g:RadioButton name="new name" text="Shared Interactive 3D Listing" checked="true" ui:field="rbShared"/>
					</g:VerticalPanel>
					<g:HorizontalPanel width="100%" height="40px"/>
					<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER" ui:field="vpFloorplan">
						<g:HTML wordWrap="true" ui:field="htmlFloorplanUrl"/>
					</g:VerticalPanel>
					<g:HorizontalPanel width="100%" height="40px"/>
				</g:VerticalPanel>
								</g:Cell>
						</g:VerticalPanel>
						</g:Cell>
			</g:VerticalPanel>
					</g:ScrollPanel>
				</g:center>
				<g:south size=".0">
					<g:HorizontalPanel spacing="10">
						<g:Cell width="100%">
							<g:Button text="Cancel" ui:field="button" enabled="false"/>
						</g:Cell>
						<g:Cell width="100%">
							<g:Button text="Submit" ui:field="buttonSubmit" enabled="false"/>
						</g:Cell>
					</g:HorizontalPanel>
				</g:south>
			</g:DockLayoutPanel>
</ui:UiBinder> 